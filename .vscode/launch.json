    "version": "0.2.0",
    "configurations": [
        {
            "name": "Manager (master)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manager/main.py",
            "args": [],
            "console": "integratedTerminal",
            // "preLaunchTask": "userProc",
            "justMyCode": false,
            // "sudo": true
        },
        {
            "name": "Manager (slave)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manager/main.py",
            "args": [
                "--no-gui",
                "--bind", "0.0.0.0:4433",
                "--insecure_bind", "0.0.0.0:8080",
                "--config", "/etc/unitotem/unitotem2.conf",
                "--remote", "wss://127.0.0.1/remote"
            ],
            "console": "integratedTerminal",
            // "preLaunchTask": "userProc",
            "justMyCode": false,
            // "sudo": true
        },
        {
            "name": "Electron",
            "program": "${workspaceFolder}/webview/main.js",
            "request": "launch",
            "runtimeExecutable": "${workspaceFolder}/webview/node_modules/.bin/electron",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        }
    ]
}